{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "impliedStrict": true
    }
  },
  "rules": {
    "no-console": 0,
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "atom": true
  }
}
